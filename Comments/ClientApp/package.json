{
  "name": "comments",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    "extends @nice-digital/browserslist-config"
  ],
  "engines": {
    "node": ">=8.11.1",
    "npm": ">=5.7.1"
  },
  "scripts": {
    "start": "rimraf ./build && npm run modernizr && npm-run-all -p sass:watch start-js",
    "start-js": "react-scripts start",
    "build": "npm run modernizr && npm run sass:build && npm run bless && react-scripts build",
    "test": "react-scripts test --env=node",
    "bless": "blessc ./public/styles/main.css --output ./public/styles/main-ie9.css",
    "eslint": "node node_modules/eslint/bin/eslint 'src/'",
    "esw": "node node_modules/eslint-watch/bin/esw 'src/' --watch",
    "test:ci": "cross-env CI=true react-scripts test --env=jsdom --testResultsProcessor=jest-teamcity-reporter",
    "test:ci:coverage": "npm run test:ci -- --coverage ",
    "eject": "react-scripts eject",
    "flow": "flow",
    "modernizr": "modernizr -c modernizr-config.json -d ./public/vendor/modernizr-custom.js",
    "sass:build": "npm-run-all -s sass:compile sass:post bless",
    "sass:compile": "node-sass-chokidar --include-path ./node_modules/@nice-digital/design-system/src/stylesheets --include-path ./node_modules/@nice-digital/icons/dist --source-map true src/main.scss -o public/styles",
    "sass:post": "postcss public/styles/*.css --use autoprefixer -d public/styles",
    "sass:watch": "npm run sass:build && chokidar \"./src/**/*.scss\" -c \"npm run sass:build\""
  },
  "dependencies": {
    "aspnet-prerendering": "^3.0.1",
    "axios": "^0.18.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-register": "^6.26.0",
    "bless": "^3.0.3",
    "bowser": "^1.9.3",
    "classlist-polyfill": "^1.2.0",
    "domelementtype": "^1.3.0",
    "domhandler": "^2.4.2",
    "domutils": "^1.7.0",
    "htmlparser2": "^3.9.2",
    "ignore-styles": "^5.0.1",
    "lodash": "^4.17.10",
    "lodash-deeper": "^1.1.0",
    "moment": "^2.22.1",
    "niceorg-client": "github:nhsevidence/niceorg-client#1.7.0.34-r3FF073C",
    "object-hash": "^1.3.0",
    "query-string": "4.3.4",
    "rc-tooltip": "^3.7.2",
    "react": "^16.3.2",
    "react-aria-live": "^2.0.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-html-parser": "^2.0.2",
    "react-lines-ellipsis": "^0.10.2",
    "react-moment": "^0.7.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-hash-link": "^1.2.0",
    "react-scrollspy": "^3.3.5",
    "react-sticky": "^6.0.2",
    "stringify-object": "^3.2.2",
    "xml-parse-from-string": "^1.0.1"
  },
  "devDependencies": {
    "@nice-digital/browserslist-config": "^1.0.0",
    "@nice-digital/design-system": "^0.5.1",
    "@nice-digital/eslint-config": "^1.0.2",
    "@nice-digital/icons": "^1.5.1",
    "axios-mock-adapter": "^1.15.0",
    "chokidar-cli": "^1.2.0",
    "cross-env": "^5.1.5",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.9.1",
    "eslint-watch": "^3.1.5",
    "flow": "^0.2.3",
    "flow-bin": "^0.75.0",
    "jest-teamcity-reporter": "^0.9.0",
    "jsdom": "^11.10.0",
    "modernizr": "^3.5.0",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^5.0.0",
    "prettier-eslint": "^8.8.1",
    "raf": "^3.4.0",
    "react-scripts": "1.1.4",
    "rimraf": "^2.6.2"
  },
  "proxy": "http://niceorg",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebookincubator/create-react-app).",
  "main": "index.js",
  "keywords": [],
