{
	"name": "comments",
	"version": "0.1.0",
	"private": true,
	"browserslist": [
		"extends @nice-digital/browserslist-config"
	],
	"scripts": {
		"start": "rimraf ./build && npm run modernizr && npm-run-all -p sass:watch start-js",
		"start-js": "react-scripts start",
		"build": "npm run modernizr && react-scripts build",
		"test": "react-scripts test --env=node",
		"eslint": "node node_modules/eslint/bin/eslint 'src/'",
		"esw": "node node_modules/eslint-watch/bin/esw 'src/' --watch",
		"test:ci": "cross-env CI=true react-scripts test --env=jsdom --testResultsProcessor=jest-teamcity-reporter",
		"test:ci:coverage": "npm run test:ci -- --coverage ",
		"eject": "react-scripts eject",
		"flow": "flow",
		"modernizr": "modernizr -c modernizr-config.json -d ./public/vendor/modernizr-custom.js",
		"// CRA+SASS": "https://github.com/facebook/create-react-app/tree/master/packages/react-scripts/template#adding-a-css-preprocessor-sass-less-etc",
		"sass:build": "npm-run-all -p sass:compile sass:post",
		"sass:compile": "node-sass-chokidar --include-path ./node_modules/@nice-digital/design-system/src/stylesheets --include-path ./node_modules/@nice-digital/icons/dist --source-map true src/main.scss -o public",
		"sass:post": "postcss public/*.css --use autoprefixer -d public",
		"sass:watch": "npm run sass:build && chokidar \"./src/**/*.scss\" -c \"npm run sass:build\""
	},
	"dependencies": {
		"@nice-digital/browserslist-config": "^1.0.0",
		"@nice-digital/design-system": "^0.4.2",
		"@nice-digital/eslint-config": "^1.0.1",
		"@nice-digital/icons": "^1.4.3",
		"aspnet-prerendering": "^3.0.1",
		"axios": "^0.18.0",
		"axios-mock-adapter": "^1.14.1",
		"babel-eslint": "^8.2.2",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react-app": "^3.1.1",
		"babel-register": "^6.26.0",
		"chokidar-cli": "^1.2.0",
		"cross-env": "^5.1.3",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"enzyme-to-json": "^3.3.1",
		"eslint": "^4.18.1",
		"eslint-config-react-app": "^2.1.0",
		"eslint-plugin-flowtype": "^2.45.0",
		"eslint-plugin-import": "^2.8.0",
		"eslint-plugin-jsx-a11y": "^5.1.1",
		"eslint-plugin-react": "^7.7.0",
		"flow": "^0.2.3",
		"flow-bin": "^0.66.0",
		"flush-promises": "^1.0.0",
		"ignore-styles": "^5.0.1",
		"isomorphic-fetch": "^2.2.1",
		"jest-teamcity-reporter": "^0.9.0",
		"jsdom": "^11.6.2",
		"modernizr": "^3.5.0",
		"moment": "^2.21.0",
		"node-sass-chokidar": "^1.2.2",
		"npm-run-all": "^4.1.2",
		"postcss-cli": "^5.0.0",
		"raf": "^3.4.0",
		"react": "^16.3.2",
		"react-dom": "^16.3.2",
		"react-scripts": "1.1.4",
		"react-helmet": "^5.2.0",
		"react-moment": "^0.7.0",
		"react-redux": "^5.0.7",
		"react-router": "^4.2.0",
		"react-router-dom": "^4.2.2",
		"react-router-hash-link": "^1.2.0",
		"react-scrollspy": "^3.3.5",
		"react-sticky": "^6.0.1",
		"redux": "^3.7.2",
		"redux-thunk": "^2.2.0",
		"rimraf": "^2.6.2"
	},
	"devDependencies": {
		"eslint-watch": "^3.1.4",
		"prettier-eslint": "^8.8.1",
		"stringify-object": "^3.2.2"
	},
	"proxy": "http://test.nice.org.uk"
}
